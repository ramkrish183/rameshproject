<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:jee="http://www.springframework.org/schema/jee" xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd  
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd  
       http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.0.xsd  
       http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">

	<bean id="objPPServiceIntf"
		class="com.ge.trans.pp.services.asset.service.impl.PPAssetServiceImpl">
		<constructor-arg ref="objPPAssetBOIntf"></constructor-arg>
	</bean>
	<bean id="objProximityServiceIntf"
		class="com.ge.trans.pp.services.proximity.service.impl.ProximityServiceImpl">
		<constructor-arg ref="objProximityBOIntf"></constructor-arg>
	</bean>
	<bean id="objNotificationServiceIntf"
		class="com.ge.trans.pp.services.notification.service.impl.NotificationServiceImpl">
		<constructor-arg ref="objNotificationBOIntf"></constructor-arg>
	</bean>

	<bean id="objIdleReportServiceIntf"
		class="com.ge.trans.pp.services.idlereport.service.impl.IdleReportServiceImpl">
		<constructor-arg ref="objIdleReportBOIntf"></constructor-arg>
	</bean>

	<bean id="objGeofenceReportServiceIntf"
		class="com.ge.trans.pp.services.geofencereport.service.impl.GeofenceReportServiceImpl">
		<constructor-arg ref="objGeofenceReportBOIntf"></constructor-arg>
	</bean>
	<bean id="objManageGeofenceServiceIntf"
		class="com.ge.trans.pp.services.manageGeofence.service.impl.ManageGeofenceServiceImpl">
		<constructor-arg ref="objManageGeofenceBOIntf"></constructor-arg>
	</bean>
	<!-- Added for Config Geofence Screen -->
	<bean id="objConfigGeofenceServiceIntf"
		class="com.ge.trans.pp.services.manageGeofence.service.impl.ConfigGeofenceServiceImpl">
		<constructor-arg ref="objConfigGeofenceBOIntf"></constructor-arg>
	</bean>
</beans>
<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://kep/kepservices/monitoring"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://kep/kepservices/monitoring"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<complexType name="knowledgeSeekerReqRequestType">
		<sequence>
			<element name="customerIds" type="xs:string" minOccurs="0" />
			<element name="fleets" type="xs:string" minOccurs="0" />
			<element name="models" type="xs:string" minOccurs="0" />
			<element name="configs" type="xs:string" minOccurs="0" />
			<element name="unitNos" type="xs:string" minOccurs="0" />
			<element name="roadNo" type="xs:string" minOccurs="0" />
			<element name="fromdate" type="xs:string" minOccurs="0" />
			<element name="todate" type="xs:string" minOccurs="0" />

			<element name="runname" type="xs:string" minOccurs="0" />


			<element name="userName" type="xs:string" minOccurs="0" />

			<element name="classification" type="xs:string" minOccurs="0" />
			<element name="filterFlag" type="xs:string" minOccurs="0" />
			<element name="classificationWindow" type="xs:string"
				minOccurs="0" />
			<element name="daysAfterEvent" type="xs:string" minOccurs="0" />
			<element name="progWindow" type="xs:string" minOccurs="0" />
			<element name="algorithm" type="xs:string" minOccurs="0" />
			<element name="symptom" type="xs:string" minOccurs="0" />
			<element name="rootCause" type="xs:string" minOccurs="0" />
			<element name="coverage" type="xs:string" minOccurs="0" />
			<element name="normalData" type="xs:string" minOccurs="0" />
			<element name="faultCode" type="xs:string" minOccurs="0" />
			<element name="runAgainst" type="xs:string" minOccurs="0" />

		</sequence>
	</complexType>

	<complexType name="knowledgeSeekerReqResponseType">
		<sequence>
			<element name="trackingID" type="xs:int" minOccurs="0" />
		</sequence>
	</complexType>

	<element name="knowledgeSeekerReqRequestType" type="tns:knowledgeSeekerReqRequestType" />
	<element name="knowledgeSeekerReqResponseType" type="tns:knowledgeSeekerReqResponseType" />

</schema>
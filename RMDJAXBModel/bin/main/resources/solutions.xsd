				minOccurs="1" />
			<element name="language" type="xs:string" minOccurs="1" />
		</sequence>
	</complexType>
	<complexType name="solutionRequestType">
		<sequence>
			<element name="solutionDetail" type="tns:solutionDetailType" />
			<element name="assetImpact" type="xs:string" minOccurs="1" />
			<element name="solutionType" type="xs:string" minOccurs="1" />
			<element name="solutionHistory" type="tns:solutionHistoryType"
				minOccurs="1" />
			<element name="taskDetail" type="tns:taskDetailType"
				maxOccurs="unbounded" />
			<element name="solutionPlot" type="tns:solutionPlotType"
				maxOccurs="unbounded" /> 
		</sequence>
	</complexType>
	<complexType name="solutionDetailType">
		<sequence>
			<element name="solutionID" type="xs:string" minOccurs="1" />

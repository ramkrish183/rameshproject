	xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://omd/omdservices/solutions"
	elementFormDefault="qualified" attributeFormDefault="unqualified">

	<complexType name="solutionFeedbackRequestType">
		<sequence>
			<element name="caseID" type="xs:string" minOccurs="1" />
			<element name="solutionID" type="xs:string" minOccurs="1" />

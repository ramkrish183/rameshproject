		</sequence>
	</complexType>
	<complexType name="solutionConfigType">
		<sequence>
			<element name="configFunction" type="xs:string" minOccurs="1" />
			<element name="configName" type="xs:string" minOccurs="1" />
			<element name="configValue1" type="xs:string" minOccurs="1"></element>
			<element name="configValue2" type="xs:string" minOccurs="1" />
		</sequence>
	</complexType>
	<!-- Added for Sprint 14 Fleet View basic story -->

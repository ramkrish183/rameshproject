			<element name="author" type="xs:string" minOccurs="1" />
			<element name="locomotiveImpact" type="xs:string" minOccurs="1" />
			<element name="model" type="xs:string" minOccurs="1" />
			<element name="solutionType" type="xs:string" minOccurs="1" />
			<element name="createdBy" type="xs:string" minOccurs="1" />
			<element name="creationDate" type="xs:dateTime" minOccurs="1" />
			<element name="lastEdited" type="xs:string" minOccurs="1" />
			<element name="lastEditedDate" type="xs:dateTime" minOccurs="1" />
			<element name="noOfActiveWeeks" type="xs:long" minOccurs="1" />

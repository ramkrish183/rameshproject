<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://omd/omdservices/lhr" xmlns:tns="http://omd/omdservices/lhr"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	
	<complexType name="vehicleHealthDataListResponse">
		<sequence>
			<element name="vehicleHealthDataList" type="tns:vehicleHealthDataResponse"  maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<complexType name="vehicleHealthDataResponse">
		<sequence>
			<element name="customerId" type="xs:string" minOccurs="1" />
			<element name="roadNumber" type="xs:string" minOccurs="1" />
			<element name="fleetId" type="xs:string" minOccurs="1" />
			<element name="modelId" type="xs:string" minOccurs="1" />
			<element name="healthScore" type="xs:float" minOccurs="1" />
			<element name="defectLevelCode" type="xs:string" minOccurs="1" />
			<element name="confidence" type="xs:double" minOccurs="1" />
			<element name="calculationDate" type="xs:dateTime" minOccurs="1" />
			<element name="componentHealthList" type="tns:performanceHealthDataResponse"  maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<complexType name="performanceHealthDataResponse">
		<sequence>
			<element name="performanceCheckName" type="xs:string" minOccurs="1" />
			<element name="performanceCheckCode" type="xs:string" minOccurs="1" />
			<element name="healthScore" type="xs:float" minOccurs="1" />
			<element name="confidence" type="xs:double" minOccurs="1" />
			<element name="calculationDate" type="xs:dateTime" minOccurs="1" />
			<element name="mostCriticalRx" type="tns:recommendationResponse"  maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<complexType name="recommendationResponse">
		<sequence>
			<element name="urgency" type="xs:string" minOccurs="1" />
			<element name="title" type="xs:string" minOccurs="1" />
			<element name="estimatedRepairTime" type="xs:string" minOccurs="1" />
			<element name="impactDescription" type="xs:string" minOccurs="1" />
			<element name="openedOn" type="xs:dateTime" minOccurs="1" />
			<element name="deliveredOn" type="xs:dateTime" minOccurs="1" />
			<element name="closedOn" type="xs:dateTime" minOccurs="1" />
		</sequence>
	</complexType>
		
	<complexType name="healthRuleFiringRequestType">
		<sequence>
			<element name="locomotiveId" type="xs:long" minOccurs="1" />
			<element name="healthRules" type="tns:healthRules"  maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="healthRules">
		<sequence>
			<element name="ruleId" type="xs:long" minOccurs="1" />
		</sequence>
	</complexType>
	<complexType name="healthRuleResponse">
		<sequence>
			<element name="ruleId" type="xs:long" minOccurs="1" />
			<element name="hasFired " type="xs:boolean" minOccurs="1" />
			<element name="runDate" type="xs:dateTime" minOccurs="1" />
		</sequence>
	</complexType>
	
	<complexType name="healthRulesFiringResponseType">
		<sequence>
			<element name="locomotiveId" type="xs:long" minOccurs="1" />
			<element name="healthRules" type="tns:healthRuleResponse"  maxOccurs="unbounded"/>
		</sequence>
	</complexType>

	<xs:element name="ruleTitle" type="xs:string" />
	<xs:element name="ruleType" type="xs:string" />
	<xs:element name="family" type="xs:string" />

	<complexType name="healthRulesResponse">
		<sequence>
			<element name="ruleId" type="xs:long" minOccurs="1" />
			<element name="ruleTitle" minOccurs="1" />
			<element name="ruleType" minOccurs="1" />
			<element name="family" minOccurs="1" />
		</sequence>
	</complexType>
	
	
	<complexType name="healthRulesRequest">
		<sequence>
			<element name="locomotives" type="tns:locomotives"  maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<complexType name="locomotives">
		<sequence>
			<element name="locomotiveId" type="xs:long" minOccurs="1" />
			<element name="healthRules" type="tns:healthRules"  maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
	<complexType name="locomotiveCommunicationResponse">
		<sequence>
			<element name="locomotiveId" type="xs:long" minOccurs="1" />
			<element name="roadNumber" type="xs:string" minOccurs="1" />
			<element name="vehicleHeader" type="xs:string" minOccurs="1" />
			<element name="nextMaintenanceType" type="xs:long" minOccurs="1" />
			<element name="nextMaintenanceDate" type="xs:string" minOccurs="1" />
			<element name="lastFaultMsgDate" type="xs:string" minOccurs="1" />
			<element name="lastATSMsgDate" type="xs:string" minOccurs="1" />
			<element name="lastTOStatusMsgDate" type="xs:string" minOccurs="1" />
		</sequence>
	</complexType>
	
	<element name="recommendationResponse" type="tns:recommendationResponse" />
	<element name="performanceHealthDataResponse" type="tns:performanceHealthDataResponse" />
	<element name="vehicleHealthDataResponse" type="tns:vehicleHealthDataResponse" />
	<element name="vehicleHealthDataListResponse" type="tns:vehicleHealthDataListResponse" />
	
	<element name="healthRuleFiringRequestType" type="tns:healthRuleFiringRequestType" />
	<element name="healthRuleRequest" type="tns:healthRules" />
	<element name="healthRuleResponse" type="tns:healthRuleResponse" />
	<element name="healthRulesFiringResponseType" type="tns:healthRulesFiringResponseType" />
	
	<element name="healthRulesResponse" type="tns:healthRulesResponse" />
	
	<element name="locomotiveCommunicationResponse" type="tns:locomotiveCommunicationResponse" />
	
</schema>
<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	targetNamespace="http://omd/omdservices/geofencereport"
	xmlns:georeport="http://omd/omdservices/geofencereport">

	<xs:element name="customerId" type="xs:string" />
	<xs:element name="roadInitial" type="xs:string" />
	<xs:element name="fromRoadNo" type="xs:string" />
	<xs:element name="toRoadNo" type="xs:string" />
	<xs:element name="fromDate" type="xs:dateTime" />
	<xs:element name="toDate" type="xs:dateTime" />
	<xs:element name="timeZone" type="xs:string" />
	<xs:element name="fleets" type="xs:string" />
	<xs:element name="geofences" type="xs:string" />

	<xs:element name="roadNumber" type="xs:string" />
	<xs:element name="geofenceName" type="xs:string" />
	<xs:element name="entryTime" type="xs:dateTime" />
	<xs:element name="totalTime" type="xs:string" />
	<xs:element name="entryFuelLevel" type="xs:string" />
	<xs:element name="totalFuel" type="xs:string" />
	<xs:element name="fleet" type="xs:string" />

	<xs:complexType name="geofenceReportListRequestType">
		<xs:sequence>
			<xs:element name="geofenceRequestTypeList" type="georeport:geofenceReportRequestType"
				maxOccurs="unbounded" />
			<xs:element ref="georeport:geofences" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="geofenceReportRequestType">
		<xs:sequence>
			<xs:element ref="georeport:customerId" minOccurs="1" />
			<xs:element ref="georeport:roadInitial" minOccurs="1" />
			<xs:element ref="georeport:fromRoadNo" minOccurs="1" />
			<xs:element ref="georeport:toRoadNo" minOccurs="1" />
			<xs:element ref="georeport:fromDate" minOccurs="1" />
			<xs:element ref="georeport:toDate" minOccurs="1" />
			<xs:element ref="georeport:timeZone" minOccurs="0" />
			<xs:element ref="georeport:fleets" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="geofenceReportResponseType">
		<xs:sequence>
			<xs:element ref="georeport:roadInitial" minOccurs="1" />
			<xs:element ref="georeport:roadNumber" minOccurs="1" />
			<xs:element ref="georeport:geofenceName" minOccurs="1" />
			<xs:element ref="georeport:entryTime" minOccurs="1" />
			<xs:element ref="georeport:totalTime" minOccurs="1" />
			<xs:element ref="georeport:entryFuelLevel" minOccurs="1" />
			<xs:element ref="georeport:totalFuel" minOccurs="1" />
			<xs:element ref="georeport:fleet" minOccurs="1" />
		</xs:sequence>
	</xs:complexType>


</xs:schema>
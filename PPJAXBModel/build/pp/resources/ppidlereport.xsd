<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	targetNamespace="http://omd/omdservices/ppidlereport" xmlns:ppidlerep="http://omd/omdservices/ppidlereport">

	<xs:element name="elapsedTime" type="xs:int" />
	<xs:element name="idleType" type="xs:string" />
	<xs:element name="totalRepotedCnt" type="xs:int" />
	<xs:element name="notMovingCnt" type="xs:int" />
	<xs:element name="notMovingEngineOnCnt" type="xs:int" />
	<xs:element name="notMovingEngineOffCnt" type="xs:int" />
	<xs:element name="dwellCnt" type="xs:int" />
	<xs:element name="engineOnCnt" type="xs:int" />
	<xs:element name="orderBy" type="xs:int" />
	<xs:element name="notMovingDuration" type="xs:float" />
	<xs:element name="engineDuration" type="xs:float" />
	<xs:element name="engineState" type="xs:string" />
	<xs:element name="dwellDuration" type="xs:float" />
	<xs:element name="distance" type="xs:string" />
	<xs:element name="location" type="xs:string" />
	<xs:element name="customerId" type="xs:string" />
	<xs:element name="stateCode" type="xs:string" />
	<xs:element name="idleSince" type="xs:dateTime" />
	<xs:element name="lastMsgTime" type="xs:dateTime" />
	<xs:element name="region" type="xs:string" />
	<xs:element name="subRegion" type="xs:string" />
	<xs:element name="roadInitialName" type="xs:string" />
	<xs:element name="roadInitialNo" type="xs:string" />
	<xs:element name="productNames" type="xs:string" />

	<xs:complexType name="idleReportDetailResponseRowType">
		<xs:sequence>
			<xs:element ref="ppidlerep:region" minOccurs="0" />
			<xs:element ref="ppidlerep:subRegion" minOccurs="0" />
			<xs:element ref="ppidlerep:roadInitialName" minOccurs="0" />
			<xs:element ref="ppidlerep:roadInitialNo" minOccurs="0" />
			<xs:element ref="ppidlerep:notMovingDuration" minOccurs="0" />
			<xs:element ref="ppidlerep:engineDuration" minOccurs="0" />
			<xs:element ref="ppidlerep:engineState" minOccurs="0" />
			<xs:element ref="ppidlerep:dwellDuration" minOccurs="0" />
			<xs:element ref="ppidlerep:distance" minOccurs="0" />
			<xs:element ref="ppidlerep:location" minOccurs="0" />
			<xs:element ref="ppidlerep:stateCode" minOccurs="0" />
			<xs:element ref="ppidlerep:idleSince" minOccurs="0" />
			<xs:element ref="ppidlerep:lastMsgTime" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="idleReportDetailResponseType">
		<xs:sequence>
			<xs:element name="idleReportDetailResponseRow"
				type="ppidlerep:idleReportDetailResponseRowType" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="idleReportDetailResponse" type="ppidlerep:idleReportDetailResponseType" />

	<xs:complexType name="idleReportSummaryResponseRowType">
		<xs:sequence>
			<xs:element ref="ppidlerep:region" minOccurs="0" />
			<xs:element ref="ppidlerep:totalRepotedCnt" minOccurs="0" />
			<xs:element ref="ppidlerep:notMovingCnt" minOccurs="0" />
			<xs:element ref="ppidlerep:notMovingEngineOnCnt"
				minOccurs="0" />
			<xs:element ref="ppidlerep:notMovingEngineOffCnt"
				minOccurs="0" />
			<xs:element ref="ppidlerep:dwellCnt" minOccurs="0" />
			<xs:element ref="ppidlerep:engineOnCnt" minOccurs="0" />
			<xs:element ref="ppidlerep:orderBy" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="idleReportSummaryResponseType">
		<xs:sequence>
			<xs:element name="idleReportSummaryResponseRow"
				type="ppidlerep:idleReportSummaryResponseRowType" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="idleReportSummaryResponse" type="ppidlerep:idleReportSummaryResponseType" />

	<xs:complexType name="idleReportRequestType">
		<xs:sequence>
			<xs:element ref="ppidlerep:customerId" minOccurs="1" />
			<xs:element ref="ppidlerep:region" minOccurs="1" />
			<xs:element ref="ppidlerep:elapsedTime" minOccurs="1" />
			<xs:element ref="ppidlerep:productNames" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="idleReportSummaryRequest" type="ppidlerep:idleReportRequestType" />

	<xs:complexType name="idleReportDetailType">
		<xs:complexContent>
			<xs:extension base="ppidlerep:idleReportRequestType">
				<xs:sequence>
					<xs:element ref="ppidlerep:idleType" minOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>

	</xs:complexType>

	<xs:element name="idleReportDetailRequest" type="ppidlerep:idleReportDetailType" />

</xs:schema>
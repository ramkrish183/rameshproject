<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	targetNamespace="http://omd/omdservices/ppnotification" xmlns:ppnotf="http://omd/omdservices/ppnotification">

	<xs:element name="roadInitialName" type="xs:string" />
	<xs:element name="roadInitialNo" type="xs:string" />
	<xs:element name="roadNo" type="xs:string" />
	<xs:element name="fromDate" type="xs:dateTime" />
	<xs:element name="toDate" type="xs:dateTime" />
	<xs:element name="notificationType" type="xs:string" />
	<xs:element name="inactiveNotification" type="xs:boolean" />
	<xs:element name="cityName" type="xs:string" />
	<xs:element name="stateCode" type="xs:string" />
	<xs:element name="region" type="xs:string" />
	<xs:element name="customerId" type="xs:string" />
	<xs:element name="timeZone" type="xs:string" />
	<xs:element name="comment" type="xs:string" />
	<xs:element name="email" type="xs:string" />
	<xs:element name="status" type="xs:string" />
	<xs:element name="milepost" type="xs:string" />
	<xs:element name="currentRegion" type="xs:string" />
	<xs:element name="subRegion" type="xs:string" />
	<xs:element name="fuelLevel" type="xs:string" />
	<xs:element name="fuelAdded" type="xs:string" />
	<xs:element name="model" type="xs:string" />
	<xs:element name="notificationDate" type="xs:dateTime" />
	<xs:element name="ppService" type="xs:boolean" />
	<xs:element name="ppAssetServiceStatus" type="ppnotf:assetServiceStatusEnum" />
	<xs:element name="productNames" type="xs:string" />

	<xs:complexType name="ppAssetServiceStatusRequestType">
		<xs:sequence>
			<xs:element ref="ppnotf:customerId" minOccurs="1" />
			<xs:element ref="ppnotf:roadInitialName" minOccurs="1" />
			<xs:element ref="ppnotf:roadNo" minOccurs="1" />
			<xs:element ref="ppnotf:ppService" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="ppAssetServiceStatusRequest" type="ppnotf:ppAssetServiceStatusRequestType" />

	<xs:simpleType name="assetServiceStatusEnum">
		<xs:restriction base="xs:string">
			<xs:enumeration value="assetNotInstalled" />
			<xs:enumeration value="serviceNotEnabled" />
			<xs:enumeration value="serviceEnabled" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="ppAssetServiceStatusResponseType">
		<xs:sequence>
			<xs:element ref="ppnotf:ppAssetServiceStatus" minOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="ppAssetServiceStatusResponse" type="ppnotf:ppAssetServiceStatusResponseType" />

	<xs:complexType name="regionType">
		<xs:sequence>
			<xs:element ref="ppnotf:region" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="regions" type="ppnotf:regionType" />

	<xs:complexType name="roadInitialType">
		<xs:sequence>
			<xs:element ref="ppnotf:roadInitialName" minOccurs="1" />
			<xs:element ref="ppnotf:roadInitialNo" minOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="roadInitial" type="ppnotf:roadInitialType" />

	<xs:complexType name="ppNotificationRequestType">
		<xs:sequence>
			<xs:element ref="ppnotf:customerId" minOccurs="1" />
			<xs:element ref="ppnotf:roadInitialName" minOccurs="0" />
			<xs:element ref="ppnotf:roadNo" minOccurs="0" />
			<xs:element ref="ppnotf:fromDate" minOccurs="1" />
			<xs:element ref="ppnotf:toDate" minOccurs="1" />
			<xs:element ref="ppnotf:notificationType" minOccurs="0" />
			<xs:element ref="ppnotf:inactiveNotification" minOccurs="1" />
			<xs:element ref="ppnotf:region" minOccurs="1" />
			<xs:element ref="ppnotf:timeZone" minOccurs="1" />
			<xs:element ref="ppnotf:productNames" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="ppNotificationRequest" type="ppnotf:ppNotificationRequestType" />

	<xs:complexType name="ppNotificationResponseType">
		<xs:sequence>
			<xs:element ref="ppnotf:roadInitialName" minOccurs="1" />
			<xs:element ref="ppnotf:roadNo" minOccurs="0" />
			<xs:element ref="ppnotf:comment" minOccurs="1" />
			<xs:element ref="ppnotf:notificationType" minOccurs="1" />
			<xs:element ref="ppnotf:email" minOccurs="1" />
			<xs:element ref="ppnotf:status" minOccurs="1" />
			<xs:element ref="ppnotf:cityName" minOccurs="1" />
			<xs:element ref="ppnotf:stateCode" minOccurs="1" />
			<xs:element ref="ppnotf:milepost" minOccurs="1" />
			<xs:element ref="ppnotf:currentRegion" minOccurs="1" />
			<xs:element ref="ppnotf:subRegion" minOccurs="1" />
			<xs:element ref="ppnotf:fuelLevel" minOccurs="1" />
			<xs:element ref="ppnotf:fuelAdded" minOccurs="1" />
			<xs:element ref="ppnotf:model" minOccurs="1" />
			<xs:element ref="ppnotf:notificationDate" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RestrictedAlertShopRespType">
		<xs:sequence>
			<xs:element name="proximityCheckFlag" type="xs:string"
				minOccurs="0" />
			<xs:element name="alertId" type="xs:string" minOccurs="0" />
			<xs:element name="customerId" type="xs:string" minOccurs="0" />
			<xs:element name="alertName" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ShopsResponseType">
		<xs:sequence>
			<xs:element name="shopId" type="xs:string" minOccurs="0" />
			<xs:element name="shopName" type="xs:string" minOccurs="0" />
			<xs:element name="customerId" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="SubDivisionResponseType">
		<xs:sequence>
			<xs:element name="subDivisionId" type="xs:string"
				minOccurs="0" />
			<xs:element name="subDivisionName" type="xs:string"
				minOccurs="0" />
			<xs:element name="regionId" type="xs:string" minOccurs="0" />
			<xs:element name="customerId" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ppNotificationResponse" type="ppnotf:ppNotificationResponseType" />

</xs:schema>
	</cache>
	<cache name="RMDNotifyFlagsCache" eternal="false"
		maxElementsInMemory="50" timeToIdleSeconds="3600" timeToLiveSeconds="86400" />
	<cache name="RMDThresholdCache" eternal="false"
		maxElementsInMemory="25" timeToIdleSeconds="3600" timeToLiveSeconds="86400" />
	<cache name="RMDStateProvinceCache" eternal="false"

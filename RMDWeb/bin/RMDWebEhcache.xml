	<cache name="RMDAllRegionsCache" eternal="false"
		maxElementsInMemory="100" timeToIdleSeconds="3600" timeToLiveSeconds="86400" />
	<cache name="CustLookupCache" eternal="false"
		maxElementsInMemory="100" timeToIdleSeconds="3600" timeToLiveSeconds="86400" />
